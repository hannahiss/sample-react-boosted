{
  "name": "create-next-production-app",
  "version": "0.1.0",
  "description": "A production-grade Next.js starter kit bootstrapped with `create-next-app`.",
  "private": true,
  "homepage": "https://github.com/yannickcornaille/create-next-production-app",
  "repository": "github.com:yannickcornaille/create-next-production-app",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test --expect 200 'yarn start' http://0.0.0.0:3000 'yarn cypress'",
    "e2e:headless": "start-server-and-test --expect 200 'yarn start' http://0.0.0.0:3000 'yarn cypress:headless'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.1",
    "next": "13.0.6",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.31.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "cypress": "^12.3.0",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "start-server-and-test": "^1.15.2"
  }
}
