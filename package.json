{
  "name": "create-next-production-app",
  "version": "0.1.0",
  "description": "A production-grade Next.js starter kit bootstrapped with `create-next-app`.",
  "private": true,
  "homepage": "https://github.com/yannickcornaille/create-next-production-app",
  "repository": "github.com:yannickcornaille/create-next-production-app",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:css": "stylelint \"**/*.js\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "e2e": "start-server-and-test 'yarn start' http://0.0.0.0:3000 'yarn cypress'",
    "e2e:headless": "start-server-and-test 'yarn start' http://0.0.0.0:3000 'yarn cypress:headless'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "next": "13.2.3",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.31.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "cypress": "^12.7.0",
    "eslint": "^8.35.0",
    "eslint-config-next": "13.2.3",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-testing-library": "^5.10.2",
    "husky": "^8.0.0",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "lint-staged": "^13.1.2",
    "postcss-styled-syntax": "^0.3.3",
    "prettier": "^2.8.4",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.2.0",
    "stylelint-config-standard": "^30.0.1",
    "typescript": "^4.9.5"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache",
      "stylelint"
    ]
  }
}
